<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:width="300dp" android:height="300dp">
    <shape android:shape="oval">
        <solid android:color="@color/colorAccent" />
    </shape>
</item>
<item android:top="75dp" android:left="80dp" android:width="50dp" android:height="150dp">
    <shape android:shape="rectangle">
        <solid android:color="@color/white" />
        <corners android:radius="4dp" />
    </shape>
</item>
<item android:top="75dp" android:right="80dp" android:width="50dp" android:height="150dp" android:gravity="right">
    <shape android:shape="rectangle">
        <solid android:color="@color/white" />
        <corners android:radius="4dp" />
    </shape>
</item>

</layer-list>